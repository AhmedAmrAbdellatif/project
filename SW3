int sw3_button;//= register ely han7oto feeh ;
int timer_last_read;
int Timer;//dh el register ely by3mel display ll time in sec. 3ala el lcd

void cont_count3()
{
	counter = timer_last_read;
	delaays_display(counter)
}




int pause_count3()
{
	
	
	while(sw3_button == 0x00)
	{NVIC_ST_CTRL_R &= ~0x1;
	}
	cont_count3();
	return(timer_last_read);
}




void sw3()// cheak if sw3 is pressed
{
	if (sw3_button == 0x0)
	{

		pause_count3();
	}



}
